{
  "mcpServers": {
    "weather": {
      "command": "npx",
      "args": [
        "-y",
        "git+https://github.com/astrix-security/mcp-secret-wrapper",
        "ACCUWEATHER_API_KEY={PASTE_THE_NEW_SECRET_ARN_HERE}",
        "--",
        "npx",
        "-y",
        "@timlukahorstmann/mcp-weather"
      ],
      "env": {
        "VAULT_TYPE": "aws",
        "VAULT_REGION": "us-east-2"
      }
    }
  }
}
